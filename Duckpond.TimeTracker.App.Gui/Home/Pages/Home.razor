@page "/"
@rendermode InteractiveServer


<PageTitle>Home</PageTitle>

<h1>Hello, world!</h1>

Welcome to your new app.

<MudButton Variant="Variant.Filled" OnClick="OnClick">Click Me: @Count</MudButton>

@code
{
    private int Count { get; set; } = 0;

    private void OnClick()
    {
        ++Count;
    }
}
